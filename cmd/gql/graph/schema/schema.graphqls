# GraphQL schema example
#
# https://gqlgen.com/getting-started/

"""
Todo represents todo type
"""
type Todo {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

"""
User represents user type
"""
type User {
  id: ID!
  name: String!
}

"""
NewTodo represents new todo input
"""
input NewTodo {
  text: String!
  userId: String!
}


